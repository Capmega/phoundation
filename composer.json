{
  "name": "capmega/phoundation",
  "description": "The phoundation PHP framework",
  "require": {
    "php": "^8.1.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-yaml": "*",
    "ext-mbstring": "*",
    "ext-posix": "*",
    "ext-fileinfo": "*",
    "mongodb/mongodb":"^1.6",
    "mrclay/minify": "*",
    "ext-redis": "*",
    "ext-mongodb": "*",
    "ext-memcached": "*",
    "ext-apache": "*"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "php-console/php-console": "@stable",
    "squizlabs/php_codesniffer": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "@stable"
  },"autoload": {
    "psr-4": {
      "Phoundation\\": "Phoundation/",
      "Plugins\\": "Plugins/",
      "Templates\\": "Templates/"
    }
  }
}

